[
  {
    "name": "console",
    "image": "cloudworkstation/console:latest",
    "cpu": 256,
    "memory": 256,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
          "awslogs-group": "/ecs/${cluster}/${service}",
          "awslogs-region": "${region}",
          "awslogs-stream-prefix": "console"
      }
    }
  }
]
